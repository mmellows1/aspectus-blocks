{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "aspectus-blocks/roi-calculator",
  "version": "0.1.0",
  "title": "Roi Calculator",
  "category": "widgets",
  "icon": "smiley",
  "description": "The official Aspectus ROI Calculator. You can test it out here as well as the frontend!",
  "example": {},
  "attributes": {
    "percentageIncrease": {
      "type": "string",
      "default": "Percentage Increase"
    },
    "hours": {
      "type": "string",
      "default": "Hours"
    },
    "days": {
      "type": "string",
      "default": "Days"
    },
    "weeksPerYear": {
      "type": "string",
      "default": "Weeks per year"
    },
    "unitsPerHour": {
      "type": "string",
      "default": "Units per hour"
    },
    "profitPerYear": {
      "type": "string",
      "default": "Profit per year"
    },
    "unitsPerYear": {
      "type": "string",
      "default": "Units per year"
    },
    "profitPerUnit": {
      "type": "string",
      "default": "Profit per unit"
    },
    "baseCurrency": {
      "type": "string",
      "default": "GBP"
    },
    "defaultDays": {
      "type": "number",
      "default": 5
    },
    "defaultHours": {
      "type": "number",
      "default": 8
    },
    "unitStep": {
      "type": "number",
      "default": 0.1
    }
  },
  "supports": {
    "html": false,
    "align": true
  },
  "textdomain": "roi-calculator",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}
